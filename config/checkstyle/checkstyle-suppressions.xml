<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- Lombokアノテーションがついたクラスの全体的なJavadocチェックを抑制 -->
    <suppress checks="JavadocMethod" files=".*Builder\.java"/>
    <suppress checks="MissingJavadocMethod" files=".*Builder\.java"/>
</suppressions> 